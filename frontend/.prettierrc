{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "endOfLine": "auto",
  "singleQuote": true,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 120,
  "bracketSpacing": true,
  "arrowParens": "always",
  "importOrder": [
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@/pages/(.*)$",
    "^@/components/(.*)$",
    "^@/apis/(.*)$",
    "^@/hooks/(.*)$",
    "^@/queries/(.*)$",
    "^@/utils/(.*)$",
    "^@/mocks/(.*)$",
    "^@/constants/(.*)$",
    "^@/assets/(.*)$",
    "^@/styles/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
